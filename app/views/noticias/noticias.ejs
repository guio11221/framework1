<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8">
	<meta name="viewport" content="width-device-width,initial-scale-1">
	<title>Portal de notícias</title>
</head>
<body>
<h1>Notícias</h1>
<% if(dados.length == ""){ %>

	<h1>sem noticias</h1>

	<% }else %>
	<table>
		<tr>
			<td>ID</td>
			<td>Título</td>
			<td>Notícia</td>
		</tr>
<% for(var i = 0; i < dados.length ;i++){ %>
		<tr>
			<td>
				<%= dados[i].id_noticia  %>
			</td>
			<td>
				<%= dados[i].titulo  %>
			</td>
			<td>
				<%= dados[i].noticia  %>
			</td>
		</tr>
	</table>

<%} %>


















</body>
</html>
